{
  "layout": {
    "type": "grid",
    "rows": 2,
    "cols": 2,
    "space": "3mm",
    "vbackbone": "5mm",
    "vbonecut": false,
    "hbackbone": "5mm",
    "hbonecut": false
  },
   "framing": {
    "type": "tightframe",
    "width": "8mm",
    "space": "3mm",
    "slotwidth": "3mm",
    "fillet": "1mm"
  },
  "source": {
    "type": "auto",
    "tolerance": "20mm"
  },
  "tabs": {
    "type": "annotation"
  },
  "cuts": {
    "type": "mousebites",
    "drill": "0.5mm",
    "spacing": "0.6mm",
    "offset": "0.2mm",
    "prolong": "0.2mm"
  },
  "fiducials": {
    "type": "4fid",
    "hoffset": "4mm",
    "voffset": "3mm",
    "coppersize": "2mm",
    "opening": "1mm",
    "paste": true
  },
  "tooling": {
    "type": "4hole",
    "size": "3mm",
    "hoffset": "4mm",
    "voffset": "7mm",
    "paste": true
  },
  "text": {
    "type": "simple",
    "text": "Pixel Pump Mainboard Rev 8",
    "anchor": "mt",
    "voffset": "4mm",
    "hjustify": "center",
    "vjustify": "center"
  },
  "copperfill": {
    "type": "hatched",
    "clearance": "0.75mm",
    "spacing": "0.75mm",
    "width": "0.75mm"
  },
  "post": {
    "millradius": "1mm"
  }
}
